{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/bids/src",
  "monorepo": true,
  "root": "apps/bids",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/bids/tsconfig.app.json"
  },
  "projects": {
    "bids": {
      "type": "application",
      "root": "apps/bids",
      "entryFile": "main",
      "sourceRoot": "apps/bids/src",
      "compilerOptions": {
        "tsConfigPath": "apps/bids/tsconfig.app.json"
      }
    },
    "event": {
      "type": "application",
      "root": "apps/event",
      "entryFile": "main",
      "sourceRoot": "apps/event/src",
      "compilerOptions": {
        "tsConfigPath": "apps/event/tsconfig.app.json"
      }
    },
    "repository": {
      "type": "application",
      "root": "apps/repository",
      "entryFile": "main",
      "sourceRoot": "apps/repository/src",
      "compilerOptions": {
        "tsConfigPath": "apps/repository/tsconfig.app.json"
      }
    },
    "client": {
      "type": "application",
      "root": "apps/client",
      "entryFile": "main",
      "sourceRoot": "apps/client/src",
      "compilerOptions": {
        "tsConfigPath": "apps/client/tsconfig.app.json"
      }
    },
    "types": {
      "type": "library",
      "root": "libs/types",
      "entryFile": "index",
      "sourceRoot": "libs/types/src",
      "compilerOptions": {
        "tsConfigPath": "libs/types/tsconfig.lib.json"
      }
    },
    "raft-node": {
      "type": "application",
      "root": "apps/raft-node",
      "entryFile": "main",
      "sourceRoot": "apps/raft-node/src",
      "compilerOptions": {
        "tsConfigPath": "apps/raft-node/tsconfig.app.json"
      }
    }
  }
}